            p1:layout_width="wrap_content"
            p1:layout_height="match_parent"
            p1:id="@+id/textView2"
            p1:layout_weight="0.6"
            p1:layout_gravity="center"
            p1:gravity="center"
            p1:textColor="#ffffff"
            p1:textSize="16sp"
            p1:textStyle="bold" />
        <ImageButton
            p1:src="@drawable/refresh"
            p1:layout_width="32dp"
            p1:layout_height="32dp"
            p1:id="@+id/standingUpdate"
            p1:layout_gravity="right"
            p1:background="#00000000"
            p1:layout_weight="0.2" />
    </LinearLayout>
